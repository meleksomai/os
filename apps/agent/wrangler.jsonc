{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "emailbot",
  "main": "./index.ts",
  "compatibility_date": "2025-12-23",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "send_email": [
    {
      "name": "SEB",
      "destination_address": "<EMAIL_ROUTING_DESTINATION>"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "HelloEmailAgent",
        "name": "HelloEmailAgent"
      }
    ]
  },
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "migrations": [
    {
      "new_sqlite_classes": ["HelloEmailAgent"],
      "tag": "v1"
    }
  ]
}
