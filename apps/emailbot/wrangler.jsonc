{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "emailbot",
  "main": "./index.ts",
  "compatibility_date": "2025-12-23",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true,
    "head_sampling_rate": 1
  },
  "send_email": [
    {
      "name": "SEB"
    }
  ],
  "durable_objects": {
    "bindings": [
      {
        "class_name": "HelloEmailAgent",
        "name": "HelloEmailAgent"
      }
    ]
  },
  "ai": {
    "binding": "AI",
    "remote": true
  },
  "migrations": [
    {
      "new_sqlite_classes": ["HelloEmailAgent"],
      "tag": "v1"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "EMAIL_LOOKUP_KV",
      "id": "5c8cd4b3d71e403384d0dd2b58e00f5a"
    }
  ]
}
